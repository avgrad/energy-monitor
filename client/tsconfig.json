{
    "extends": "../tsconfig.json",
    "include": ["./"],
    "compilerOptions": {
        "baseUrl": "./",
        "rootDir": "./",
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "esModuleInterop": false,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "types": ["vite/client"]
    }
}
